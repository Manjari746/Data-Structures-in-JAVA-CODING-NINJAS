public class solution {

	public static String addStars(String s) {
        int n=s.length();
		if(n==1)
        return s;
        
       else if (s.charAt(0)==s.charAt(1))
        return s.charAt(0)+"*"+addStars(s.substring(1));
       else
        return s.charAt(0)+addStars(s.substring(1));
        

	}
}
