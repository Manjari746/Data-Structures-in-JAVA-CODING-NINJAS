public class Solution {
    
    public static int max_number(int n){


int n1=n;
int count=4;
int max=-1;
int b=1;
while(count-->0){
n1=((n/(10*b)) *b) + n%b;
max=Math.max(n1,max);
n1=n;
b*=10;
}
return max;



// int n1=n;
//  n1=(n1/10)*1+n%1;

// int n2=n;
// n2=((n/100)*10)+n%10;


// int n3=n;

// n3=((n/1000)*100)+n%100;

// int n4=n;

// n4= ((n/10000)*1000)    +   n%1000;

// return Math.max(n1,Math.max(n2,Math.max(n3,n4)));

    

}
}
